{
  "name": "@unfocused/nurvus-ui",
  "version": "0.1.0-alpha.0",
  "description": "NURVUS Design System",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "rimraf dist && npm run build:tokens && webpack --mode=production --json > stats.json",
    "build:tokens": "node ./tools/build-tokens.js",
    "prepare": "npm run build",
    "styleguide": "npm run build:tokens && styleguidist server --config styleguide/webpack.config.js",
    "styleguide:build": "rimraf styleguide/build && npm run build:tokens && styleguidist build --config styleguide/webpack.config.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "classnames": "^2.3.1",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-styleguidist": "^11.1.7",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "typescript": "^4.4.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UnfocusedDrive/nurvus-design-system.git"
  },
